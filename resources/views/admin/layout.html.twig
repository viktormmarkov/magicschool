<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

 {% include 'AppBundle:Admin:header.html.twig' %}
 {% include 'AppBundle:Admin:navigation.html.twig' %}

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                {% block pageHeading %}{% endblock pageHeading %}
                <small>{% block pageHeadingDescription %} {% endblock pageHeadingDescription %}</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="{{ path("app_admin_dashboard") }}"><i class="fa fa-dashboard"></i> {% trans  %}admin.dashboard.dashboard{% endtrans %}</a></li>
                <li class="active">{% block breadcrumb %} {% endblock breadcrumb %}</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">


            <div class="box box-default color-palette-box">
                <div class="box-header with-border">
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-lg-12">
                            {% block body %}
                            {% endblock body %}
                        </div>
                        <!-- col-lg-12 -->
                    </div><!-- /.row -->
                </div><!-- /.box-body -->
            </div><!-- /.box -->



        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->

{% include 'AppBundle:Admin:footer.html.twig' %}
{% include 'AppBundle:Admin:aside-control.html.twig' %}

